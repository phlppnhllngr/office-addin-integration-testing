{
  "name": "office-addin-integration-testing",
  "version": "1.0.0",
  "description": "Integration testing Office Add-ins with Appium and Node.js",
  "main": "index.js",
  "scripts": {
    "dev-server": "cd %npm_package_config_addIn% && npm run dev-server",
    "sideload": "cd %npm_package_config_addIn% && npm run sideload",
    "appium": "appium --port %npm_package_config_port% --address %npm_package_config_address%",
    "suite": "node index.js",
    "e2e": "npm-run-all sideload suite",
    "postinstall": "cd %npm_package_config_addIn% && npm install",
    "servers": "run-p dev-server appium"
  },
  "config": {
    "addIn": "My Office Add-in",
    "port": 4723,
    "address": "127.0.0.1"
  },
  "keywords": [
    "Appium",
    "WinAppDriver",
    "Office Add-in",
    "TaskPane Add-in",
    "e2e",
    "end-to-end",
    "integration"
  ],
  "author": "phlppnhllngr",
  "license": "NONE",
  "devDependencies": {
    "generator-office": "^1.2.4",
    "yo": "^2.0.6"
  },
  "dependencies": {
    "nan": "^2.14.0",
    "windows-build-tools": "^5.1.0",
    "appium": "^1.13.0",
    "colors": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "wd": "^1.11.2",
    "winctl": "^1.0.2"
  }
}
